{
  "esversion": 6,
  "camelcase": true,
  "indent": 2,
  "maxlen": 180,
  "trailing": true,
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "undef": true,
  "unused": true,
  "devel": true,
  "node": false,
  "sub": true,
  "lastsemic": false,
  "quotmark": "single",
  "predef": [
    // standard
    "navigator",
    "document",
    "window",
    "console",
    "alert",
    "setTimeout",
    "toString",
    "isNaN",
    // nodejs
    "module",
    "require",
    "process",
    // jasmine
    "jasmine",
    "expect",
    "fail",
    "spyOn",
    "describe",
    "it",
    "before",
    "beforeEach",
    "after",
    "afterEach",
    // video.js
    "videojs",
    "YT",
    "Image",
    //SOUNDCLOUD
    "SC",
    //DEEZER
    "DZ"
  ]
}

